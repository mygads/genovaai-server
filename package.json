{
  "name": "genovaai-server",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:8090": "next dev -p 8090",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "start:8090": "next start -p 8090",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed-genovaai.ts",
    "db:reset": "prisma migrate reset --force && tsx prisma/seed-genovaai.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "jose": "^6.1.2",
    "lucide-react": "^0.554.0",
    "mammoth": "^1.11.0",
    "next": "16.0.3",
    "pdf-parse": "^2.4.5",
    "prisma": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed-genovaai.ts"
  }
}
